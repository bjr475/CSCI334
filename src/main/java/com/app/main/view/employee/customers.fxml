                                            <TableColumn prefWidth="600" text="Address">
                                            </TableColumn>
                                            <TableColumn prefWidth="200" text="Contact Name">
                                            </TableColumn>
                                            <TableColumn prefWidth="600" text="Details">
                                            </TableColumn>
                                        </columns>
                                    </TableView>
                                </content>
                            </ScrollPane>
                        </center>
                        <right>
