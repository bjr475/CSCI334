                                                    </content>
                                                </TitledPane>
                                                <HBox>
                                                    <Pane HBox.hgrow="ALWAYS"/>
                                                    <JFXButton>
                                                        Search
                                                    </JFXButton>
                                                </HBox>
                                            </VBox>
                                        </content>
                                    </ScrollPane>
                                </center>
                            </BorderPane>
                        </right>
